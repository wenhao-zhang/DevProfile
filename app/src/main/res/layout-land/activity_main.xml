<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView11"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:background="@color/colorAccent"/>

    <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="40dp"
            android:layout_marginTop="80dp"
            app:layout_constraintTop_toTopOf="parent"/>

    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView11"
            app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginStart="28dp"
            android:textSize="24sp"/>

    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView19"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintHorizontal_bias="0.8"
            app:layout_constraintEnd_toStartOf="@+id/imageView20"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintTop_toTopOf="@+id/imageView20"
            app:layout_constraintBottom_toBottomOf="@+id/imageView20"/>

    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView20"
            app:layout_constraintStart_toEndOf="@+id/imageView19"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView21"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            app:layout_constraintTop_toTopOf="@+id/imageView21"
            app:layout_constraintBottom_toBottomOf="@+id/imageView21"/>

    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView21"
            app:layout_constraintStart_toEndOf="@+id/imageView20"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView11"/>

    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView26"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView27"
            android:scaleType="centerCrop"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/logo"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="32dp"/>

    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView27"
            app:layout_constraintStart_toEndOf="@+id/imageView26"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView28"
            android:scaleType="centerCrop"
            app:layout_constraintTop_toTopOf="@+id/imageView26"
            app:layout_constraintBottom_toBottomOf="@+id/imageView26"
            app:layout_constraintDimensionRatio="1:1"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>

    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView28"
            app:layout_constraintStart_toEndOf="@+id/imageView27"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView29"
            android:scaleType="centerCrop"
            app:layout_constraintTop_toTopOf="@+id/imageView27"
            app:layout_constraintBottom_toBottomOf="@+id/imageView27"
            app:layout_constraintDimensionRatio="1:1"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>

    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView29"
            app:layout_constraintStart_toEndOf="@+id/imageView28"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            android:scaleType="centerCrop"
            app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28"
            app:layout_constraintDimensionRatio="h,1:1"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="32dp"/>
</android.support.constraint.ConstraintLayout>